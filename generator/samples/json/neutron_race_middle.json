{
    "name": "test",
    "timeout": 180,
    "source": {
        "batch_size": 512,
        "concurency": 4,
        "interval": 100,
        "batch_number": 10000,
        "fields": [
            {
                "name": "value",
                "type": "int",
                "limit": [
                    0,
                    10000
                ]
            },
            {
                "name": "time",
                "type": "timestamp",
                "timestamp_format": "2006-01-02 15:04:05.000000"
            }
        ]
    },
    "sinks": [
        {
            "type": "neutron",
            "properties": {
                "address": "http://localhost:8000"
            }
        }
    ],
    "observer": {
        "type": "neutron",
        "properties": {
            "address": "http://localhost:8000",
            "query": "select * from test where value=9",
            "time_column": "time",
            "metric": "latency"   
        }
    }
}
